class Solution {
public:
    void setZeroes(vector<vector<int>>& matrix) {
        int n=matrix.size();
        int m=matrix[0].size();
        
        set<int>s1,s2;
        
        for(int row=0;row<n;row++)
        {
            for(int col=0;col<m;col++)
            {
                if(matrix[row][col]==0)
                {
                   s1.insert(row);
                   s2.insert(col);
                }
            }
        }
        
        for(int row=0;row<n;row++)
        {
            for(int col=0;col<m;col++)
            {
                if(s1.find(row)!= s1.end() || s2.find(col)!= s2.end())
                    matrix[row][col]=0;
            }
        }
    }
};
